<?xml version="1.0" encoding="UTF-8"?>

        <?import javafx.scene.control.Button?>
        <?import javafx.scene.control.Label?>
        <?import javafx.scene.control.SplitPane?>
        <?import javafx.scene.layout.AnchorPane?>
        <?import javafx.scene.layout.VBox?>
        <?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ufc.br.controller.UserController">
    <children>
        <SplitPane focusTraversable="true" VBox.vgrow="ALWAYS">
            <items>
                <AnchorPane>
                    <children>
                        <Label alignment="CENTER" layoutX="355.0" layoutY="36.0" text="PAINEL DO USUÃRIO">
                            <font>
                                <Font name="System Bold" size="31.0" />
                            </font>
                        </Label>

                        <Button defaultButton="true" layoutX="323.0" layoutY="153.0" mnemonicParsing="false"
                                onAction="#irParaListaTrabalhos" prefHeight="86.0" prefWidth="225.0" text="TRABALHOS" />

                        <Button defaultButton="true" layoutX="323.0" layoutY="248.0" mnemonicParsing="false"
                                onAction="#irParaNovoTrabalho" prefHeight="86.0" prefWidth="225.0" text="CADASTRAR TRABALHO" />

                        <Button layoutX="824.0" layoutY="14.0" mnemonicParsing="false"
                                onAction="#deslogar" style="-fx-background-color: red;" text="SAIR" textFill="WHITE" />
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </children>
</VBox>